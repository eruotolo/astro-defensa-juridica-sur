---
import { SliderHome } from "../components/slider/SliderHome";
import Layout from "../layouts/Layout.astro";
import About from "../components/About.astro";
import Service from "../components/Service.astro";
---

<Layout>
    <div class="mt-[-120px]" id="inicio">
        <SliderHome client:load />
    </div>

    <!-- NOSTROS -->
    <section id="nosotros" class="py-[50px]">
        <About />
    </section>

    <!-- SERVICIOS -->
    <section class="bg-gris-bg py-[50px]" id="servicios">
        <Service />
    </section>
</Layout>
